{
  "name": "generator-web-application",
  "version": "3.1.2",
  "description": "Scaffold your next HTML5 app with webpack and webpack dev server.",
  "homepage": "https://github.com/jeantimex/generator-web-application",
  "author": {
    "name": "jeantimex",
    "email": "jean.timex@gmail.com",
    "url": "https://github.com/jeantimex"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-xo": "^0.29.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "release-it": "^12.6.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.1.0"
  },
  "engines": {
    "npm": ">= 6.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^4.5.0",
    "chalk": "^3.0.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "__tests__/.*\\.js$"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "release": "npm run test && release-it"
  },
  "repository": "jeantimex/generator-web-application",
  "license": "MIT"
}
